<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ydzhao.weixin.tuisong.mapper.JiNianRiMapper">
    <select id="select" resultType="ydzhao.weixin.tuisong.pojo.JiNianRi">
        select * from tb_jinianri where name = #{name};
    </select>
    <update id="update">
        update tb_jinianri set date = #{date} where name = #{name};
    </update>
</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ydzhao.weixin.tuisong.mapper.JinJuMapper">
    <select id="select" resultType="ydzhao.weixin.tuisong.pojo.JinJu">
        select * from tb_jinju where id = #{id};
    </select>
    <select id="count" resultType="int">
        select count(*) from tb_jinju;
    </select>
</mapper>